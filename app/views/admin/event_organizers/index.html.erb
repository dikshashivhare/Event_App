<p id="notice"><%= notice %></p>

<h1>Event Organizers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Logo image</th>
      <th>Contact</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_organizers.each do |event_organizer| %>
      <tr>
        <td><%= event_organizer.name %></td>
        <td><%= event_organizer.email %></td>
        <td><%= event_organizer.address %></td>
        <td><%= event_organizer.city %></td>
        <td><%= event_organizer.state %></td>
        <td><%= event_organizer.country %></td>
        <td><%= event_organizer.latitude %></td>
        <td><%= event_organizer.longitude %></td>
        <td><%= event_organizer.logo_image %></td>
        <td><%= event_organizer.contact %></td>
        <td><%= event_organizer.slug %></td>
        <td><%= link_to 'Show', admin_event_organizer %></td>
        <td><%= link_to 'Edit', edit_admin_event_organizer_path(event_organizer) %></td>
        <td><%= link_to 'Destroy', admin_event_organizer_path(event_organizer), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Organizer', new_event_organizer_path %>
